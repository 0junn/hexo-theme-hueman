<aside id="sidebar">
  <div class="sidebar-top">
    <i id="icon-angle-double-right" class="toggle icon"></i>
    <div class="sidebar-top-inner">
      <p>Follow:</p>
      <ul class="social-links">
        <% for (var i in theme.social_links){ %>
          <li><a class="social-tooltip" title="<%= i %>" href="<%- url_for(theme.social_links[i]) %>"><i id="icon-<%= i %>" class="icon"></i></a></li>
        <% } %>
      </ul>
    </div>
  </div>
  <% if (is_post()){ %>
    <%- partial('post/nav', {post: page}) %>
  <% } %>
  <div class="widgets-container">
    <% theme.widgets.forEach(function(widget){ %>
      <%- partial('_widget/' + widget) %>
    <% }) %>
  </div>
</aside>